{
  "name": "svite-example-routify-mdsvex-typescript",
  "version": "0.0.0",
  "scripts": {
    "validate": "svelte-check && tsc --noEmit",
    "dev": "run-p dev:routify dev:svite",
    "dev:routify": "routify run",
    "dev:svite": "svite -ts",
    "build": "run-s build:routify build:svite",
    "build:routify": "routify run -b",
    "build:svite": "svite build -ts"
  },
  "dependencies": {
    "svelte": "3.24.1",
    "svelte-hmr": "0.10.2",
    "@sveltech/routify": "1.9.7"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.8",
    "mdsvex": "^0.8.2",
    "npm-run-all": "^4.1.5",
    "svelte-check": "^1.0.2",
    "svelte-preprocess": "^4.0.10",
    "svite": "latest",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7"
  }
}
